<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.devanshgandhi.mediu_doctor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Home" />

        <activity
            android:name=".Appointments"
            android:parentActivityName=".Home" />

        <activity
            android:name=".HealthFeed"
            android:parentActivityName=".Home" />

        <activity
            android:name=".Chat"
            android:parentActivityName=".Home" />

        <activity
            android:name=".DoctorAccountSettings"
            android:parentActivityName=".Home" />

        <activity
            android:name=".DoctorLogin"
            android:parentActivityName=".MainActivity"/>

        <activity
            android:name=".DoctorRegister"
            android:parentActivityName=".MainActivity" />

        <activity
            android:name=".Profile"
            android:parentActivityName=".DoctorAccountSettings" />

        <activity
            android:name=".ProfilePicture"
            android:parentActivityName=".Profile"/>

        <activity
            android:name=".ForgetPassword"
            android:parentActivityName=".DoctorLogin"/>

        <activity
            android:name=".PasswordChange"
            android:parentActivityName=".DoctorAccountSettings" />
    </application>

</manifest>